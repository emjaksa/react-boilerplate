{
  "name": "react-boilerplate",
  "version": "0.0.0",
  "engines": {
    "node": ">=6.13.1",
    "npm": ">=3.10.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emjaksa/react-boilerplate.git"
  },
  "author": "Evan Jaksa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emjaksa/react-boilerplate/issues"
  },
  "homepage": "https://github.com/emjaksa/react-boilerplate#readme",
  "scripts": {
    "precommit": "run-p lint:fix test",
    "test": "cross-env NODE_ENV=production jest",
    "test:watch": "jest --watch",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "stylelint": "stylelint **/*.{css,scss,sass}",
    "stylelint:fix": "stylelint **/*.{css,scss,sass} --fix",
    "lint": "run-p eslint stylelint",
    "lint:fix": "run-p eslint:fix stylelint:fix",
    "clean": "rimraf build/ analytics/ */**/__generated__/",
    "prettier": "prettier --write **/*.js",
    "bundle": "webpack --mode=production",
    "bundle:dev": "webpack --mode=development",
    "build": "npm-run-all clean bundle",
    "dev": "npm-run-all clean test bundle:dev",
    "dev:watch:client": "webpack-dev-server --mode=development --color --open --hot --config-name=client",
    "dev:watch:server": "webpack --mode=development -w --config-name=server",
    "dev:watch": "run-p test:watch dev:watch:client dev:watch:server",
    "watch": "npm-run-all clean dev:watch",
    "start": "npm-run-all bundle && node build/server.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "^7.0.0-beta.42",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0-beta.42",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@babel/preset-react": "^7.0.0-beta.42",
    "@babel/preset-stage-2": "^7.0.0-beta.42",
    "@babel/register": "^7.0.0-beta.42",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-pure-class-to-function": "^1.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.1.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "node-sass": "^4.8.3",
    "nodemon": "^1.17.2",
    "npm-run-all": "^4.1.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.3",
    "prettier": "^1.11.1",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "style-loader": "^0.20.3",
    "stylelint": "^9.1.3",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-no-unsupported-browser-features": "^2.0.0",
    "stylelint-webpack-plugin": "git+https://github.com/emjaksa/stylelint-webpack-plugin.git",
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "webpack-node-externals": "^1.6.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.42",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "dotenv": "^5.0.1",
    "ejs": "^2.5.8",
    "express": "^4.16.3",
    "helmet": "^3.12.0",
    "lodash": "^4.17.5",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.0",
    "normalize.css": "^8.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "serialize-javascript": "^1.4.0",
    "url-search-params-polyfill": "^3.0.0",
    "whatwg-fetch": "^2.0.3",
    "winston": "^3.0.0-rc3",
    "winston-daily-rotate-file": "^3.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
